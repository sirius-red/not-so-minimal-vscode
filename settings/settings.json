{
    "breadcrumbs.enabled": false,
    "catppuccin-noctis-icons.files.associations": {
        ".graphqlrc": "graphql",
        ".graphqlrc.config.js": "graphql",
        ".graphqlrc.config.ts": "graphql",
        ".graphqlrc.js": "graphql",
        ".graphqlrc.json": "graphql",
        ".graphqlrc.ts": "graphql",
        ".graphqlrc.yaml": "graphql",
        ".graphqlrc.yml": "graphql",
        "graphql.config.js": "graphql",
        "graphql.config.ts": "graphql",
        "graphql.config.yaml": "graphql",
        "graphql.config.yml": "graphql",
        "tsconfig.dev.json": "tsconfig"
    },
    "catppuccin-noctis-icons.folders.associations": {
        "api.server": "api",
        "electron": "server",
        "gql.server": "graphql",
        "graphql.server": "graphql",
        "renderer": "client"
    },
    "catppuccin-noctis-icons.hidesExplorerArrows": false,
    "catppuccin.syncWithIconPack": true,
    "catppuccin.workbenchMode": "flat",
    "chat.commandCenter.enabled": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.foldingImportsByDefault": true,
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.formatOnSave": true,
    "editor.guides.bracketPairs": true,
    "editor.hover.delay": 700,
    "editor.indentSize": 4,
    "editor.inlayHints.fontFamily": "JetBrains Mono",
    "editor.inlineSuggest.suppressSuggestions": true,
    "editor.largeFileOptimizations": false,
    "editor.lineHeight": 1.7,
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.quickSuggestions": {
        "strings": "on"
    },
    "editor.quickSuggestionsDelay": 700,
    "editor.renderLineHighlight": "gutter",
    "editor.semanticHighlighting.enabled": true,
    "editor.semanticTokenColorCustomizations": {
        "enabled": true
    },
    "editor.smoothScrolling": true,
    "editor.suggest.filterGraceful": true,
    "editor.suggest.insertMode": "insert",
    "editor.suggest.matchOnWordStartOnly": false,
    "editor.suggest.preview": false,
    "editor.suggest.selectionMode": "whenQuickSuggestion",
    "editor.tokenColorCustomizations": {
        "[*Dark*]": {
            "textMateRules": [
                {
                    "scope": "ref.matchtext",
                    "settings": {
                        "foreground": "#fff"
                    }
                }
            ]
        },
        "[*Light*]": {
            "textMateRules": [
                {
                    "scope": "ref.matchtext",
                    "settings": {
                        "foreground": "#000"
                    }
                }
            ]
        },
        "textMateRules": []
    },
    "explorer.compactFolders": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmPasteNative": false,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "*.ahk": "${capture}.d.ahk",
        "*.cs": "${capture}.*",
        "*.csproj": "${capture}.csproj*",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.ts": "${capture}.js, ${capture}.d.ts, ${capture}.js.map, ${capture}.min.js",
        "*.tsx": "${capture}.ts",
        "Cargo.toml": "Cargo.*, tauri.conf.*",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, tsconfig*, .eslintrc*, eslint.config.*, .prettierrc*, .babelrc*, pnpm-workspace.*, .yarnrc*, .npmrc*, tailwind.config.*, postcss.config.*, vite.config.*, prettier.config.*, rsbuild.*, .prettierignore, .eslintignore, .pnp.cjs, .pnp.loader.mjs, codegen.ts, .graphqlrc*, next.config.*, apollo.config.*, graphql.config.*"
    },
    "extensions.ignoreRecommendations": true,
    "files.associations": {
